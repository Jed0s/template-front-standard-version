{
  "name": "template-standard-version",
  "version": "1.0.0",
  "author": "jed0s",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jed0s/template-front-standard-version.git"
  },
  "scripts": {
    "dev": "next & open http://localhost:3000/",
    "tsc": "tsc -p .",
    "cm": "yarn tsc && yarn lint && git cz",
    "lint": "yarn lint:js && yarn lint:sc",
    "lint:sc": "stylelint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "lint:js": "eslint . --ext ts --ext tsx"
  },
  "dependencies": {
    "antd": "^4.9.4",
    "next": "^10.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "styled-reset": "^4.3.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-styled-components": "^1.12.0",
    "commitizen": "^4.2.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.1.3"
  }
}
